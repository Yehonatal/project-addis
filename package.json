{
    "name": "project-addis",
    "version": "1.0.0",
    "description": "Full-stack song management application with React frontend and Node.js backend for Addis software plc as my test project",
    "main": "index.js",
    "scripts": {
        "dev": "concurrently \"pnpm run backend:dev\" \"pnpm run frontend:dev\"",
        "backend:dev": "cd backend && pnpm run dev",
        "frontend:dev": "cd frontend && pnpm run dev",
        "backend:install": "cd backend && pnpm install",
        "frontend:install": "cd frontend && pnpm install",
        "install:all": "pnpm run backend:install && pnpm run frontend:install",
        "build": "cd frontend && pnpm run build",
        "start": "cd backend && pnpm start"
    },
    "keywords": [
        "react",
        "redux",
        "node",
        "express",
        "songs",
        "crud"
    ],
    "author": "yonatan afewerk",
    "license": "MIT",
    "devDependencies": {
        "@types/dotenv-webpack": "^7.0.8",
        "@types/webpack-dev-server": "^4.7.2",
        "concurrently": "^8.2.2",
        "dotenv-webpack": "^8.1.1"
    }
}
